{
    "name": "udemy_cypress",
    "version": "1.0.0",
    "description": "learning-cypress-udemy",
    "license": "ISC",
    "author": "Albarokah",
    "type": "commonjs",
    "main": "index.js",
    "scripts": {
        "cy-run": "npx cypress run",
        "cy-open": "npx cypress open",
        "cy-run-chrome": "npx cypress run --browser chrome",
        "cy-run-firefox": "npx cypress run --browser firefox",
        "cy-run-edge": "npx cypress run --browser edge",
        "cy-run-headed-chrome": "npx cypress run --headed --browser chrome",
        "cy-run-headed-firefox": "npx cypress run --headed --browser firefox",
        "cy-run-headed-edge": "npx cypress run --headed --browser edge",
        "cy-run-headless": "cypress run --headless",
        "cy-run-headless-chrome": "cypress run --headless --browser chrome",
        "cy-run-headless-firefox": "cypress run --headless --browser firefox",
        "cy-run-headless-edge": "cypress run --headless --browser edge",
        "recordDashboardTest": "dotenv -e .env npx cypress run --record",
        "cy-open-chrome": "npx cypress open --browser chrome",
        "cy-open-firefox": "npx cypress open --browser firefox",
        "cy-open-edge": "npx cypress open --browser edge",
        "TestExampleEcommers": "npx cypress run --spec \"cypress/e2e/TestExampleEcommers/*\"",
        "SmokeTest": "npx cypress run --spec \"cypress/e2e/TestExampleEcommers/Test1.cy.js/*\"",
        "cy-run-cucumber": "npx cypress run --spec \"cypress/e2e/BDD Features/**/*.feature\"",
        "cy-open-cucumber-open": "npx cypress open --e2e --browser chrome",
        "cy-run-cucumber-chrome": "npx cypress run --spec \"cypress/e2e/BDD Features/**/*.feature\" --browser chrome",
        "cy-run-cucumber-headed": "npx cypress run --spec \"cypress/e2e/BDD Features/**/*.feature\" --headed --browser chrome",
        "cy-run-cucumber-headless": "npx cypress run --spec \"cypress/e2e/BDD Features/**/*.feature\" --headless --browser chrome",
        "format": "prettier --write .",
        "format-check": "prettier --check ."
    },
    "cypress-cucumber-preprocessor": {
        "stepDefinitions": "cypress/e2e/step_definitions/**/*.{js,ts}",
        "filterSpecs": true,
        "omitFiltered": true
    },
    "devDependencies": {
        "@bahmutov/cypress-esbuild-preprocessor": "^2.2.7",
        "@esbuild-plugins/node-globals-polyfill": "^0.2.3",
        "@esbuild-plugins/node-modules-polyfill": "^0.2.2",
        "cypress": "^15.7.0",
        "cypress-iframe": "^1.0.1",
        "cypress-mochawesome-reporter": "^4.0.2",
        "dotenv": "^17.2.3",
        "dotenv-cli": "^11.0.0",
        "multiple-cucumber-html-reporter": "^3.9.3",
        "prettier": "^3.0.0"
    },
    "dependencies": {
        "@badeball/cypress-cucumber-preprocessor": "^23.2.1"
    }
}